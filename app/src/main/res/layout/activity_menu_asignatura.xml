<?xml version="1.0"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MenuCicloActivity"
    android:weightSum="1">


    <Button
        android:id="@+id/buttonAsignaturaActualizar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="lanzarActividad"
        android:text="actualizar_asignatura"
        android:layout_weight="0.10" />

    <Button
        android:id="@+id/buttonAsignaturaDelete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="lanzarActividad"
        android:text="delete_asignatura"
        android:layout_weight="0.06" />

    <Button
        android:id="@+id/buttonAsignaturaInsertar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="lanzarActividad"
        android:text="ingresar_asignatura"
        android:layout_weight="0.06" />
    <Button
        android:id="@+id/buttonAsignaturaConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="lanzarActividad"
        android:text="consultar_asignatura"
        android:layout_weight="0.06" />
</LinearLayout>

